<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <table>

      <Row>
        <Cell><Data ss:Type="String">Seed</Data></Cell>
        <Cell><Data ss:Type="String">Weight</Data></Cell>
        <Cell><Data ss:Type="String">School</Data></Cell>
        <Cell><Data ss:Type="String">First</Data></Cell>
        <Cell><Data ss:Type="String">Last</Data></Cell>
        <Cell><Data ss:Type="String">Year</Data></Cell>
        <Cell><Data ss:Type="String">W</Data></Cell>
        <Cell><Data ss:Type="String">L</Data></Cell>
      </Row>
      <% @wrestlers.each do |wrestler| %>
        <Row>
          <Cell><Data ss:Type="String"><%= wrestler.seed %></Data></Cell>
          <Cell><Data ss:Type="String"><%= wrestler.weight %></Data></Cell>
          <Cell><Data ss:Type="String"><%= wrestler.school %></Data></Cell>
          <Cell><Data ss:Type="String"><%= wrestler.first_name %></Data></Cell>
          <Cell><Data ss:Type="String"><%= wrestler.last_name %></Data></Cell>
          <Cell><Data ss:Type="String"><%= wrestler.grade %></Data></Cell>
          <Cell><Data ss:Type="String"><%= wrestler.wins %></Data></Cell>
          <Cell><Data ss:Type="String"><%= wrestler.losses %></Data></Cell>
        </Row>
      <% end %>
    </table>
  </Worksheet>
</Workbook>