
<tr>
  <td><%= user.league %></td>
  <td><%= user.name %></td>

    <td><%= link_to "Wrestlers (#{user.wrestlers.count})", user_wrestlers_path(user) %></td>
    <% if current_user.admin? %>
    <td><%= user.cell %></td>
    <td><%= user.email %></td>
    <td><%= link_to 'Edit', edit_user_path(user)%></td>
    <td><%= link_to 'Delete', user, method: "delete", :data => {:confirm => 'Are you sure?'} %>

    <% end %>

  </td>
</tr>