<div class="team-name">

  <h1>Wrestlers </h1> (<%= @wrestlers.count %>)
  <!-- <p>
    Sort by:
      <%= link_to "School", wrestlers_sort_by_school_path %> |
      <%= link_to "Weight", wrestlers_path %> |
      <%= link_to "State Place", wrestlers_sort_by_state_place_path %>
  </p> -->

  <p>
    Download:
      <%= link_to "CSV", wrestlers_path(format: "csv") %> |
      <%= link_to "Excel", wrestlers_path(format: "xls") %>


  </p>



</div>
<div class="container" ng-view="">
  <div class="row">
    <div class="panel ">
      <div class="panel-body">
        <div class="table-responsive">
          <table class="table table-bordered table-hover" id="wrestlers_table">
            <thead>
              <tr>
                <th>Wt</th>
                <th>League Place</th>
                <th>School</th>
                <th>First</th>
                <th>Last</th>
                <th>YR</th>
                <th>W</th>
                <th>L</th>
                <th>Comments</th>
                 <% if current_user.admin?  %>
                   <th class="col-md-0">Edit Wrestler</th>
                   <th class="col-md-0">Delete Wrestler</th>
                 <% end %>
              </tr>
            </thead>
            <tbody>
            <script>
              (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
              (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
              m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
              })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

              ga('create', 'UA-59923417-1', 'auto');
              ga('send', 'pageview');

            </script>
              <% @wrestlers.each do |wrestler, user| %>
                <% if wrestler.first_name != "     NO ENTRY"  %>
                  <%= render partial: '/users/wrestlers/table_row', locals: { wrestler: wrestler, user: user} %>
                <% end %>
              <% end %>
            </tbody>
          </table>
        </div>
      </div>
    </div>